@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html  ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Examen.Angular</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/Librerias/bootstrap/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>

</head>
<body ng-controller="aplicacionController as vm"
      ng-init="init('@ViewBag.WebApiUrl')">
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">Examen.Angular</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                    <li><a ui-sref="miembro" href="#" ui-sref-active="active">Participantes</a></li>
                    <li><a ui-sref="corporacion" href="#" ui-sref-active="active">Corporacion</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - Examen.Angular</p>
        </footer>
    </div>

    <environment names="Development">
        <script src="~/js/bootstrap.js"></script>
        <script src="~/js/angular.js"></script>
        <script src="~/js/app-site.js"></script>
        <script src="~/js/app-examen.js"></script>
    </environment>
</body>
</html>
